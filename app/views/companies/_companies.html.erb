<table class="table table-striped table-sm ">
  <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">Joint Stock Company</th>
        <th scope="col">Symbol</th>
        <th scope="col">Stock</th>
        <th scope="col">Shares amount</th>
        <th scope="col">Free float</th>
      </tr>
    </thead>
    <tbody>
      <% companies.each do |company| %>
        <tr>
          <td class="text-dark" ><%= company.id %></td>
          <td><%= link_to company.name, company, class: "nav-link text-dark p-0" %></td>
          <td class="text-muted" ><%= company.isin %></td>
          <td class="text-muted" >###</td>
          <td class="text-muted" >###</td>
          <td class="text-muted" >## %</td>
        </tr>
      <% end %>
    </tbody>
</table>
